{
  "name": "wordi",
  "version": "1.0.0",
  "description": "Extension for learning foreign languages",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "compose": "docker-compose -f run/config/pg-and-redis.docker-copmose.yaml up",
    "start": "ENV_FILE_PATH=run/config/dotenv/.dev.env nest start",
    "start:dev": "ENV_FILE_PATH=run/config/dotenv/.dev.env nest start --watch",
    "start:debug": "ENV_FILE_PATH=run/config/dotenv/.dev.env nest start --debug --watch",
    "webpack": "webpack -c src/front/webpack.config.js --mode production"
  },
